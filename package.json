{
  "name": "ShaderLabWeb",
  "description": "ShaderLabWeb",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "transpile": "yarn babel src --out-dir _transpiled --delete-dir-on-start --copy-files",
    "serve": "yarn transpile && yarn node --es-module-specifier-resolution=node _transpiled/serve.js",
    "init-server": "server/init.sh",
    "deploy": "server/update.sh"
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-syntax-import-assertions": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@koa/router": "^10.1.1",
    "bcrypt": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-static": "^5.0.0",
    "monaco-editor": "^0.21.3",
    "postcss": "^8.4.14",
    "postcss-nesting": "^8.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sqlite": "^4.1.1",
    "sqlite3": "^5.0.8",
    "sqlstring-sqlite": "^0.1.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3"
  }
}
