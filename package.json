{
  "name": "ShaderLabWeb",
  "description": "ShaderLabWeb",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "transpile": "yarn babel src --out-dir _transpiled --delete-dir-on-start --copy-files",
    "serve": "yarn transpile && yarn node --es-module-specifier-resolution=node _transpiled/serve.js",
    "init-server": "server/init.sh",
    "deploy": "server/update.sh"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-react": "^7.10.4",
    "@koa/router": "^10.0.0",
    "bcrypt": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-static": "^5.0.0",
    "monaco-editor": "^0.21.2",
    "postcss": "^8.2.15",
    "postcss-nesting": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "sqlstring-sqlite": "^0.1.1"
  }
}
