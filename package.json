{
  "name": "ShaderLabWeb",
  "description": "ShaderLabWeb",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "transpile": "yarn babel src --out-dir _transpiled --delete-dir-on-start --copy-files",
    "serve": "yarn transpile && yarn node --import=./_transpiled/register.js --trace-warnings _transpiled/serve.js",
    "init-server": "server/init.sh",
    "deploy": "server/update.sh"
  },
  "dependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/plugin-syntax-import-assertions": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@koa/router": "^12.0.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-favicon": "^2.1.0",
    "koa-static": "^5.0.0",
    "monaco-editor": "^0.48.0",
    "postcss": "^8.4.38",
    "postcss-nesting": "^12.1.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "resolve": "^1.22.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "sqlstring-sqlite": "^0.1.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3"
  }
}
